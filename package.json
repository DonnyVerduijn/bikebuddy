{
  "name": "bikebuddy",
  "displayName": "bikebuddy",
  "version": "1.0.0",
  "description": "A hybrid application to locate stored bicycles",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DonnyVerduijn/bikebuddy.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "run": "cordova run android",
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "remotedev": "remotedev --hostname=localhost --port=8000"
  },
  "author": "Donny Verduijn",
  "license": "Apache-2.0",
  "dependencies": {
    "@google/maps": "^0.5.5",
    "@reactivex/rxjs": "^6.3.3",
    "aphrodite": "^2.2.3",
    "cordova-android": "^7.1.1",
    "cordova-browser": "^5.0.4",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-googlemaps": "git+https://github.com/mapsplugin/cordova-plugin-googlemaps.git#multiple_maps",
    "cordova-plugin-locationservices": "^2.1.0",
    "cordova-plugin-webpack": "0.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "dotenv-webpack": "^1.5.7",
    "file-loader": "^2.0.0",
    "geolib": "^2.0.24",
    "hammerjs": "^2.0.8",
    "lodash.omit": "^4.5.0",
    "moment": "^2.22.2",
    "react": "^16.6.0",
    "react-animations": "^1.0.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-svg": "^6.0.20",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "save-dev": "^2.0.0",
    "uuidv4": "^2.0.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-locationservices": {},
      "cordova-plugin-webpack": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-googlemaps": {
        "API_KEY_FOR_ANDROID": "...",
        "API_KEY_FOR_IOS": "...",
        "PLAY_SERVICES_VERSION": "15.0.1",
        "ANDROID_SUPPORT_V4_VERSION": "27.+",
        "LOCATION_WHEN_IN_USE_DESCRIPTION": "This app wants to get your location while this app runs only.",
        "LOCATION_ALWAYS_USAGE_DESCRIPTION": "This app wants to get your location always, even this app runs in background."
      }
    },
    "platforms": [
      "android",
      "browser"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.0.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.6.2",
    "redux-logger": "^3.0.6",
    "remote-redux-devtools": "^0.5.13",
    "remotedev-server": "github:DonnyVerduijn/remotedev-server#fix-vulnerabilities",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "svg-url-loader": "^2.3.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "keywords": [
    "cordova",
    "react",
    "webpack",
    "eslint",
    "babel",
    "es2018",
    "aphrodite",
    "hybrid",
    "application"
  ],
  "bugs": {
    "url": "https://github.com/DonnyVerduijn/bikebuddy/issues"
  },
  "homepage": "https://github.com/DonnyVerduijn/bikebuddy#readme"
}